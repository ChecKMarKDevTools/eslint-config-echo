{
  "name": "@checkmarkdevtools/eslint-config-echo",
  "version": "0.0.0",
  "description": "Enterprise ESLint plugin repo that ships dual v8/v9 configs and rule presets aligned to Prettier, Sonar, and Jest workflows",
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./eslint.config.mjs",
      "require": "./index.js"
    }
  },
  "files": [
    "index.js",
    "eslint.config.mjs"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "jest",
    "prettier",
    "sonar"
  ],
  "author": "ChecKMarKDevTools",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "dependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChecKMarKDevTools/eslint-config-echo.git"
  }
}
